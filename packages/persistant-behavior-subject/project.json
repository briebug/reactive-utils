{
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/persistant-behavior-subject/src",
  "projectType": "library",
  "targets": {
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["packages/persistant-behavior-subject/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/packages/persistant-behavior-subject"],
      "options": {
        "jestConfig": "packages/persistant-behavior-subject/jest.config.ts",
        "passWithNoTests": true
      }
    },
    "build": {
      "executor": "@nrwl/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/packages/persistant-behavior-subject",
        "tsConfig": "packages/persistant-behavior-subject/tsconfig.lib.json",
        "packageJson": "packages/persistant-behavior-subject/package.json",
        "main": "packages/persistant-behavior-subject/src/index.ts",
        "assets": ["packages/persistant-behavior-subject/*.md"]
      }
    }
  },
  "tags": []
}
